
{% comment %} 
####################################################################################################
  Details index page - Blend Commerce
  Turbo version: 3.0.4
  Uses section's snippets for an improved static page WYSIWYG editor.
####################################################################################################
{% endcomment %}
<div class="detail-sections">
{% if section.blocks.size > 0 %}
  {% for block in section.blocks %}
  {% if block.settings.display_page == page.handle %}
    <div {{ block.shopify_attributes }} class="block__{{ block.type }} bc__container" data-block="block__{{ block.type }}">
      {% if block.type contains "tb__" or block.type contains "bc__"%}
      	{% include block.type, type: 'block' %}
      {% else %}
        {% assign includer = block.type | prepend: "include-" %}
        {% include includer, type: 'block'%}
      {% endif %}
    </div> 
  {% endif %}
  {% endfor %}
{% endif %}
</div>
